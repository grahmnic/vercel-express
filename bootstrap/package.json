{
  "main": "build/index.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "unseed": "sequelize db:seed:undo:all",
    "setupDB": "npm run unmigrate && npm run migrate && npm run unseed && npm run seed",
    "prebuild": "echo --- tslint -c tslint.json -p tsconfig.json --fix && npm run setupDB",
    "build": "rimraf build && tsc",
    "prestart": "npm run build",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^14.14.31",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "sequelize": "^6.12.0-beta.3",
    "sequelize-cli": "^6.3.0",
    "sqlite3": "^5.0.2"
  }
}
